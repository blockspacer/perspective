{
    "name": "@finos/perspective-workspace",
    "version": "0.4.0-rc.4",
    "description": "Perspective Workspace",
    "main": "./dist/cjs/perspective-workspace.js",
    "module": "./dist/cjs/perspective-workspace.js",
    "browser": "dist/cjs/perspective-workspace.js",
    "files": [
        "dist/**/*",
        "index.d.ts"
    ],
    "unpkg": "./dist/umd/perspective-workspace.js",
    "scripts": {
        "bench": "npm-run-all bench:build bench:run",
        "bench:build": "echo \"No Benchmarks\"",
        "bench:run": "echo \"No Benchmarks\"",
        "build:babel": "cpx \"src/**/*\" dist && babel src/js --source-maps --out-dir dist/esm",
        "build:webpack:cjs": "webpack --color --config config/cjs.config.js",
        "build:webpack:umd": "webpack --color --config config/umd.config.js",
        "build:webpack": "npm-run-all build:webpack:cjs build:webpack:umd",
        "build": "npm-run-all --silent build:babel build:webpack:cjs build:webpack:umd",
        "clean": "rimraf dist",
        "test:build": "cpx \"test/html/*\" dist/umd",
        "watch": "webpack --color --watch --config config/watch.config.js",
        "test:run": "jest --rootDir=. --config=../perspective-test/jest.config.js --silent --color 2>&1",
        "test": "echo \"No Tests\""
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/finos/perspective"
    },
    "author": "",
    "license": "Apache-2.0",
    "dependencies": {
        "@finos/perspective": "^0.4.0-rc.4",
        "@finos/perspective-phosphor": "^0.4.0-rc.4",
        "@finos/perspective-viewer": "^0.4.0-rc.4",
        "@finos/perspective-viewer-highcharts": "^0.4.0-rc.4",
        "@finos/perspective-viewer-hypergrid": "^0.4.0-rc.4",
        "@phosphor/widgets": "^1.9.3",
        "babel-runtime": "^6.26.0",
        "chroma-js": "^1.3.4",
        "core-js": "^2.6.9",
        "d3": "^5.7.0",
        "d3-svg-legend": "^2.25.6",
        "d3fc": "14.0.40",
        "gradient-parser": "0.1.5"
    },
    "devDependencies": {
        "@finos/perspective-test": "^0.4.0-rc.4",
        "babel-loader": "^8.0.6"
    }
}
